{
  "extends": "tslint:latest",
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      true,
      "never"
    ],
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "no-unused-variable": true
  }
}
